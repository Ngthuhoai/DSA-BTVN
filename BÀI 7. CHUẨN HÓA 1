#include<bits/stdc++.h>
#define ll long long
using namespace std;
int main(){
	int t;cin>>t;
	cin.ignore(); 
	while(t--){
		string s;
		getline(cin,s);
		stringstream ss(s);
		string tmp;
		string a=""; 
		while(ss>>tmp){
			
			for(int i=0;i<tmp.size();i++){
				tmp[i]=tolower(tmp[i]); 
			}
			tmp[0]=toupper(tmp[0]);
			a+=tmp+ " ";		
		} 
		cout<<a<<endl; 
		 
	} 
} 
